{"ast":null,"code":"import service from '../../service/SeleksiyonService';\nimport KasaOlculeriDetayYeni from './KasaOlculeriDetayYeni';\nimport KasaOlculeriDetay from './KasaOlculeriDetay';\nexport default {\n  components: {\n    KasaOlculeriDetayYeni,\n    KasaOlculeriDetay\n  },\n\n  created() {\n    service.getKasaDetayOlculeri().then(data => {\n      this.kasaDetaylari = data;\n    });\n  },\n\n  data() {\n    return {\n      kasaDetaylari: null,\n      is_select_new: false,\n      is_select_chest: false,\n      kasaDatas: null\n    };\n  },\n\n  methods: {\n    isSelectNew() {\n      this.is_select_new = true;\n    },\n\n    excel_cikti_click_kasa_olculeri() {\n      this.$store.dispatch('loadBegin'); //loading png gösteriyor\n\n      const data = this.kasaDetayları;\n      service.getKasaOlculeriExcelListesi(data).then(response => {\n        if (response.status) {\n          console.log(\"excell\", this.servis_adres);\n          const link = document.createElement('a');\n          link.href = this.servis_adres + 'siparisler/dosyalar/kasaOlculeriExcelCikti';\n          link.setAttribute('download', 'kasadetay_listesi.xlsx');\n          document.body.appendChild(link);\n          link.click();\n          this.is_excel = false;\n          this.$store.dispatch('loadEnd'); // loading sonlandırma png\n        }\n      });\n    },\n\n    isKasaOlcuSelect(event) {\n      console.log(event.data);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAyBA,OAAOA,OAAP,MAAoB,iCAApB;AACA,OAAOC,qBAAP,MAAkC,yBAAlC;AACA,OAAOC,iBAAP,MAA8B,qBAA9B;AACA,eAAe;EACXC,UAAU,EAAE;IACRF,qBADQ;IAERC;EAFQ,CADD;;EAKXE,OAAO,GAAG;IACNJ,OAAO,CAACK,oBAAR,GAA+BC,IAA/B,CAAoCC,IAAG,IAAK;MACxC,KAAKC,aAAL,GAAqBD,IAArB;IACH,CAFD;EAGH,CATU;;EAUXA,IAAI,GAAG;IAEH,OAAO;MACHC,aAAa,EAAE,IADZ;MAEHC,aAAa,EAAE,KAFZ;MAGHC,eAAe,EAAE,KAHd;MAIHC,SAAS,EAAC;IAJP,CAAP;EAQH,CApBU;;EAqBXC,OAAO,EAAE;IACLC,WAAW,GAAG;MACV,KAAKJ,aAAL,GAAqB,IAArB;IACH,CAHI;;IAILK,+BAA+B,GAAG;MAC9B,KAAKC,MAAL,CAAYC,QAAZ,CAAqB,WAArB,EAD8B,CACI