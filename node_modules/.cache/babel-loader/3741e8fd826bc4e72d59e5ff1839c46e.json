{"ast":null,"code":"import service from '../../service/SeleksiyonService';\nimport { mapGetters } from 'vuex';\nimport KasaOlculeriDetay from './KasaOlculeriDetay.vue';\nimport kasaDetayOlculeriYeni from './kasaDetayOlculeriYeni.vue';\nimport { eventListener } from '../../main';\nexport default {\n  components: {\n    kasaDetayOlculeriDetay: KasaOlculeriDetay,\n    kasaDetayOlculeriYeni: kasaDetayOlculeriYeni\n  },\n\n  created() {\n    service.getKasaDetayOlculeri().then(data => {\n      this.kasaDetayları = data;\n    });\n  },\n\n  data() {\n    return {\n      kasaDetayları: null,\n      is_select_chest: false,\n      kasaDatas: null,\n      is_select_new: false\n    };\n  },\n\n  computed: { ...mapGetters(['servis_adres'])\n  },\n  methods: {\n    kasaYukle() {\n      service.getKasaDetayOlculeri().then(data => {\n        this.kasaDetayları = data;\n      });\n    },\n\n    isKasaOlcuSelect(event) {\n      this.is_select_chest = true;\n      this.kasaDatas = event;\n    },\n\n    //\n    excel_cikti_click_kasa_olculeri() {\n      //verileri flask tarafına gönderiyor\n      this.$store.dispatch('loadBegin'); //loading png gösteriyor\n\n      const data = this.kasaDetayları;\n      service.getKasaOlculeriExcelListesi(data).then(response => {\n        if (response.status) {\n          console.log(\"excell\", this.servis_adres);\n          const link = document.createElement('a');\n          link.href = this.servis_adres + 'siparisler/dosyalar/kasaOlculeriExcelCikti';\n          link.setAttribute('download', 'kasadetay_listesi.xlsx');\n          document.body.appendChild(link);\n          link.click();\n          this.is_excel = false;\n          this.$store.dispatch('loadEnd'); // loading sonlandırma png\n        }\n      });\n    },\n\n    isSelectNew() {\n      this.is_select_new = true;\n    } //\n\n\n  },\n\n  mounted() {\n    eventListener.$on('kasalarLoad', data => {\n      if (data == true) {\n        window.location.reload(false);\n      }\n    });\n  }\n\